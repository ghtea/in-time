{
  "name": "InTime",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "a-d": "react-native run-android --verbose --appIdSuffix=debug",
    "a-c": "cd ./android && ./gradlew --stop && ./gradlew cleanBuildCache",
    "i-d": "react-native run-ios --verbose --configuration=Debug",
    "i-c": "rm -rf ./node_modules && yarn && rm -rf ./ios/build && rm -rf ./ios/Pods && rm -rf ./ios/Podfile.lock && cd ./ios && arch -x86_64 pod install && cd ..",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/drawer": "^6.1.4",
    "@react-navigation/elements": "^1.1.0",
    "@react-navigation/material-top-tabs": "^6.0.2",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "dayjs": "^1.10.7",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-pager-view": "^5.4.1",
    "react-native-reanimated": "^2.2.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-tab-view": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/react-native": "^0.65.0",
    "babel-jest": "^27.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^27.1.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
